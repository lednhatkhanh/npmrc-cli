{
  "name": "npmrc-cli",
  "version": "0.0.1",
  "description": "npmrc-cli CLI",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lednhatkhanh/npmrc-cli.git"
  },
